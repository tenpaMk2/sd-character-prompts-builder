---
import Layout from "../layouts/Layout.astro";
import { tabInfos, type TabKeys } from "../components/Tabs.astro";

const tabKey: TabKeys = `usage`;
const tabName = tabInfos[tabKey].ja;
const layoutInfo = {
  tabKey,
  tabName,
};
---

<Layout {...layoutInfo}>
  <article>
    <h2>ボタンを押せ</h2>
    <p>それだけ。</p>
  </article>
</Layout>
